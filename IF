    "charac_entries=['Lithology','Land use']\n",
    "target_entry='slide_state'\n",
    "#将数据集中字符型数据转换成哑变量\n",
    "charac_entries_value={}\n",
    "for w in charac_entries:\n",
    "    charac_entries_value[w]={v:k+1 for k,v in enumerate(df_data[w].unique())}\n",
    "\n",
    "print(charac_entries_value)"
   ]
