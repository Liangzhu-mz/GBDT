    "df_matrix=pd.DataFrame()\n",
    "df_label=pd.DataFrame()\n",
    "for w in df_data:\n",
    "    if w in charac_entries:\n",
    "        df_matrix[w]=df_data[w].map(charac_entries_value[w])  #直接将字符串映射到数值类型\n",
    "    elif w==target_entry:\n",
    "        df_label[w]=df_data[w]\n",
    "    elif df_data[w].dtype in [np.int64, np.float64]:\n",
    "        df_matrix[w]=df_data[w]"
